{
  "name": "gpt4-vscode-plugin",
  "displayName": "GPT-4 VSCode Plugin",
  "version": "1.1.14",
  "description": "Interacts with OpenAI GPT-4 and writes code directly in VSCode",
  "publisher": "pierorassu",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:pierorassu/gpt4-vscode-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/pierorassu/gpt4-vscode-plugin/issues"
  },
  "homepage": "https://github.com/pierorassu/gpt4-vscode-plugin#readme",
  "scripts": {
    "compile": "tsc",
    "vscode:prepublish": "npm run compile",
    "build": "webpack",
    "patch": "npm version patch",
    "major": "npm version patch",
    "minor": "npm version patch",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.9",
    "@vscode/test-electron": "^1.6.1",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-mock-imports": "^1.3.8",
    "ts-node": "^10.9.1",
    "typescript": "latest",
    "vscode": "^1.1.34",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@types/vscode": "^1.81.0",
    "openai": "^4.4.0"
  },
  "activationEvents": [
    "onCommand:extension.openGGVSCodeChat"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.openGGVSCodeChat",
        "title": "Open GPT-4 Chat"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "GGVscode Chat Configuration",
      "properties": {
        "gpt4VSCodePlugin.apiKey": {
          "type": "string",
          "default": "",
          "description": "OpenAI API Key"
        }
      }
    },
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "extension.openGGVSCodeChat",
          "group": "navigation"
        }
      ]
    }
  },
  "keywords": [
    "openai",
    "open-ai",
    "gpt4",
    "gpt-4",
    "vscode",
    "visual-studio-code"
  ],
  "license": "MIT",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleNameMapper": {
      "^vscode$": "<rootDir>/__mocks__/vscode.ts"
    }
  }
}
